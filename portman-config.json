{
  "version": 1,
  "tests": {
    "integrationTests": [
      {
        "name": "Login and List Users",
        "operations": [
          {
            "openApiOperationId": "login",
            "variations": [
              {
                "name": "defaultLogin",
                "assignVariables": [
                  {
                    "collectionVariables": [
                      {
                        "responseBodyProp": "token",
                        "name": "authToken"
                      }
                    ]
                  }
                ],
                "tests": [
                  "statusCode",
                  "contentType"
                ]
              }
            ]
          },
          {
            "openApiOperationId": "listUsers",
            "variations": [
              {
                "name": "usersWithToken",
                "overwrites": [
                  {
                    "overwriteRequestHeaders": [
                      {
                        "key": "x-api-key",
                        "value": "reqres-free-v1"
                      },
                      {
                        "key": "Authorization",
                        "value": "Bearer {{authToken}}"
                      }
                    ]
                  }
                ],
                "tests": [
                  "statusCode",
                  "contentType"
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}
